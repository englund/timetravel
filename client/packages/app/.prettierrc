{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "react",
    "^@tanstack/react-query/?(.*)$",
    "^@mui/(.*)$",
    "^notistack/?(.*)$",
    "^date-fns/?(.*)$",
    "^@fontsource/(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "trailingComma": "none"
      }
    }
  ]
}
